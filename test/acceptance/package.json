{
  "name": "ffc-future-grants-apply-acceptance-tests",
  "version": "0.32.1",
  "description": "Acceptance tests for FFC future grants apply Service",
  "homepage": "https://github.com/DEFRA/ffc-future-grants-apply",
  "keywords": [
    "wdio",
    "webdriver",
    "webdriverio",
    "cucumber",
    "test",
    "selenium"
  ],
  "scripts": {
    "test": "wdio wdio.bs.config.js",
    "test:smoke:snd": "ENV=snd wdio wdio.config.js  --cucumberOpts.tagExpression='@smoke'",
    "test:smoke:dev": "ENV=dev wdio wdio.config.js  --cucumberOpts.tagExpression='@smoke'",
    "test:smoke:test": "wdio wdio.config.js  --cucumberOpts.tagExpression='@smoke'",
    "test:smoke:pre": "ENV=pre wdio wdio.config.js  --cucumberOpts.tagExpression='@smoke'",
    "generate-report:allure": "allure generate allure-results --clean",
    "generate-report:cucumber-html": "node reporter/cucumber-report.js",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/@wdio/cli/bin/wdio.js wdio.bs.config.js"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@cucumber/cucumber": "^9.0.0",
    "@hapi/wreck": "^18.0.1",
    "@log4js-node/log4js-api": "^1.0.2",
    "@rpii/wdio-report-events": "^0.2.0",
    "@types/cucumber": "^4.0.5",
    "@wdio/browserstack-service": "^8.6.0",
    "@wdio/config": "^5.11.0",
    "@wdio/reporter": "^7.25.0",
    "@wdio/static-server-service": "^5.14.4",
    "axe-webdriverio": "^0.2.0",
    "browserstack-local": "^1.4.8",
    "chai": "^4.1.2",
    "constants": "^0.0.2",
    "cross-env": "^5.2.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "ecstatic": "^4.1.4",
    "hosted-git-info": "^4.0.2",
    "joi": "^17.9.1",
    "moment": "^2.29.4",
    "selenium": "^2.20.0",
    "selenium-server": "^3.141.59",
    "selenium-standalone": "^6.24.0",
    "wdio-geckodriver-service": "^1.0.0",
    "webdriverio": "^7.0.0",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@rpii/wdio-html-reporter": "^7.0.0",
    "@wdio/allure-reporter": "^7.16.3",
    "@wdio/cli": "^7.14.0",
    "@wdio/cucumber-framework": "^7.13.2",
    "@wdio/local-runner": "^7.13.2",
    "@wdio/spec-reporter": "^7.13.2",
    "allure-commandline": "^2.13.8",
    "babel-jest": "^24.8.0",
    "chromedriver": "^116.0.0",
    "cucumber-html-reporter": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "pngjs": "^5.0.0",
    "standard": "12.0.1",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-cucumberjs-json-reporter": "^4.1.4"
  },
  "standard": {
    "globals": [
      "browser",
      "it",
      "expect",
      "describe",
      "jest",
      "beforeEach",
      "assert",
      "$"
    ]
  }
}